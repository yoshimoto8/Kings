<% if @search_items.present? %>
    <% @search_items.each do |item| %>
      <div class="list">
        <% if item.image_url.present? %>
          <%= link_to (image_tag(item.image_url, style: "width: 70px;height: 100px;display: inline-block;")), item.url %>
        <% end %>
        <%= item.title %>
      </div>
    <% end %>
  <% end %>